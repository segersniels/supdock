{
  "name": "supdock",
  "version": "2.2.2",
  "description": "What's Up Dock(er)?",
  "main": "index.js",
  "scripts": {
    "build": "tsc --skipLibCheck ; pkg -t node10 dist/index.js -o ./bin/supdock",
    "build:ci": "tsc --skipLibCheck ; pkg dist/index.js --out-path ./bin ; ./scripts/rename.sh",
    "lint": "standard index.ts src/*.ts src/**/*.ts",
    "lint:fix": "standard --fix index.ts src/*.ts src/**/*.ts",
    "check:typescript": "tsc --noEmit --skipLibCheck --project .",
    "nuke": "rm -rf node_modules ; npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/segersniels/supdock-ts.git"
  },
  "keywords": [
    "docker",
    "supdock",
    "visual",
    "interactive",
    "wrapper",
    "cli",
    "compose",
    "docker-compose"
  ],
  "author": "Niels Segers",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/segersniels/supdock-ts/issues"
  },
  "homepage": "https://github.com/segersniels/supdock-ts#readme",
  "dependencies": {
    "chalk": "^2.4.2",
    "fuzzy-search": "^3.0.1",
    "inquirer": "^5.1.0",
    "lodash.flatten": "^4.4.0",
    "minimist": "^1.2.0"
  },
  "bin": {
    "supdock": "index.js"
  },
  "devDependencies": {
    "@types/fuzzy-search": "^2.1.0",
    "@types/inquirer": "0.0.43",
    "@types/lodash.flatten": "^4.4.6",
    "@types/node": "^11.9.4",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "pkg": "^4.4.0",
    "standard": "^14.1.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  }
}
